<article>
  <header>
    <!-- <span data-type="activity">Activity</span> -->
    <div class="participants">
      Between you and 
      
      <% users = @to_users() %>
      <% for user, i in users: %>
        <a data-id="<%= user.id %>"><%= user.toString() %></a><% if i isnt users.length - 1: %>,<% end %> 
      <% end %>
    </div>
  </header>  
  
  <div class="items"></div>
  
  <div class="compose"></div>
</article>